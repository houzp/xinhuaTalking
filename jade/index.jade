                .btn
                    .btn
                        .icon-1
                        | 三个字
                    .btn
                        .icon-2
                        | 三个字

        .swiper-container.main#main
            .swiper-wrapper                    
                .swiper-slide.main-slide(data-hash="1")
                    include mod-top-nav
                    .swiper-container.scenes-1#scenes-1
                        .swiper-wrapper
                            .swiper-slide
                                .swiper-slide-part
                                    .scenes-1-logo
